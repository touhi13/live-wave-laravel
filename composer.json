{
    "name": "livewave/laravel-sdk",
    "description": "Official Laravel SDK for LiveWave real-time events and notifications platform",
    "keywords": ["laravel", "livewave", "realtime", "websocket", "broadcasting", "notifications", "pusher", "reverb"],
    "homepage": "https://github.com/YOUR_USERNAME/live-wave-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "LiveWave Team",
            "email": "support@livewave.app"
        }
    ],
    "support": {
        "issues": "https://github.com/YOUR_USERNAME/live-wave-laravel/issues",
        "source": "https://github.com/YOUR_USERNAME/live-wave-laravel"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/broadcasting": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LiveWave\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LiveWave\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiveWave\\LiveWaveServiceProvider"
            ],
            "aliases": {
                "LiveWave": "LiveWave\\Facades\\LiveWave"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
